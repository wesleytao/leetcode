    "class Solution:\n",
    "    def arrayPairSum(self, nums):\n",
    "        \"\"\"\n",
    "        :type nums: List[int]\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        nums.sort()\n",
    "        new_array =[nums[idx] for idx in range(0,len(nums),2) ]\n",
    "        return(sum(new_array))"
   ]
